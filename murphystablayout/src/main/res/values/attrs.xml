<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MurphySTabLayout">
        <attr name="title" format="string"/>
        <attr name="titleSize" format="float"/>
        <attr name="titleColor" format="color"/>

        <attr name="leftTitle" format="string"/>
        <attr name="leftTitleSize" format="float"/>
        <attr name="leftTitleColor" format="color"/>
        <attr name="leftBackground" format="color|reference"/>

        <attr name="rightTitle" format="string"/>
        <attr name="rightTitleSize" format="float"/>
        <attr name="rightTitleColor" format="color"/>
        <attr name="rightBackground" format="color|reference"/>
    </declare-styleable>
</resources>